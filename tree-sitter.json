{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "oat",
      "camelcase": "Oat",
      "title": "oat",
      "scope": "source.oat",
      "file-types": [
        "oat"
      ],
      "injection-regex": "^oat$",
      "class-name": "TreeSitterOat",
      "highlights": "queries/oat/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Oat language used for practicing parser and lexer basics",
    "authors": [
      {
        "name": "WhySoBad",
        "email": "git@wysbd.dev"
      }
    ],
    "links": {
      "repository": "https://github.com/WhySoBad/tree-sitter-oat"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}